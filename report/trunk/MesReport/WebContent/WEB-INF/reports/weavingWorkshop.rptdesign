            <method name="beforeOpen"><![CDATA[importPackage(Packages.java.io);
importPackage(Packages.java.util);
importPackage(Packages.java.lang);

importPackage(Packages.mes.common.util);

var macro;
var replaceStr;
var currentDate = new Date();
if((params["beginDate"].value == null || params["beginDate"].value == "")&amp;&amp;(params["endDate"].value == null || params["endDate"].value == "")){
// 如果日期没选择，报表查询日期范围是：当前日期前三天的数据
	params["beginDate"].value=DateUtil.getDaysBefore(3);
	params["endDate"].value=DateUtil.getCurrentDate();
}

if (params["classGroup"].value != null &amp;&amp; params["classGroup"].value != ""){
	macro = "1 = 1" ;
	replaceStr = "weaving_record.`work_team` ='" +params["classGroup"].value+"'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
}

if (params["beginDate"].value != null &amp;&amp; params["beginDate"].value != ""){
	macro = "2 = 2" ;
	replaceStr = "weaving_record.`create_date`>='" + params["beginDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
	
	macro = "4 = 4" ;
	replaceStr = "upper.`create_date`>='" + params["beginDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
	
	macro = "6 = 6" ;
	replaceStr = "shift_info.`shift_day`>='" + params["beginDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
}

if (params["endDate"].value != null &amp;&amp; params["endDate"].value != ""){
	macro = "3 = 3" ;
	replaceStr = "weaving_record.`create_date`<='" + params["endDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
	
	macro = "5 = 5" ;
	replaceStr = "upper.`create_date`>='" + params["beginDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);

	macro = "7 = 7" ;
	replaceStr = "shift_info.`shift_day`>='" + params["beginDate"].value + "'" ;
	this.queryText = this.queryText.replaceFirst(macro,replaceStr);
}]]></method>
