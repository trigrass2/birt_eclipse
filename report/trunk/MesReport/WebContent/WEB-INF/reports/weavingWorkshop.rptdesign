            <xml-property name="queryText"><![CDATA[SELECT DISTINCT 
  weaving_record.`machine_code`,
  weaving_record.`shaft_code`,
  weaving_record.`variety`,
  weaving_record.`material_name`,
  (SELECT 
    staff.name 
  FROM
    `mes-external`.`mdm_staff_info` staff 
  WHERE staff.code = weaving_record.`weaving_worker`) weaving_worker,
  (SELECT 
    staff.name 
  FROM
    `mes-external`.`mdm_staff_info` staff 
  WHERE staff.code = weaving_record.`downer`) downer,
  SUM(weaving_record.`number`) totalNum,
  weaving_record.`work_class`,
  (SELECT 
    org.`name` 
  FROM
    `mes-external`.`mdm_org` org 
  WHERE org.code = weaving_record.`work_team`) work_team,
  (SELECT 
    org.`name` 
  FROM
    `mes-external`.`mdm_org` org 
  WHERE org.code = weaving_record.`workshop`) workshop_name,
  weaving_record.`create_date`,
  upper_latitude.`shangshaWeiNum`, 
  info.`output_weft_sum`,
  info.`output_meter_sum`,
  info.`shift_efficiency_avg` 
FROM
  `mes-job`.`job_weaving_record` weaving_record 
  LEFT JOIN 
    (SELECT DISTINCT 
      SUM(upper.`number`) AS shangshaWeiNum,
      upper.`machine_code` 
    FROM
      `mes-job`.`job_upper_latitude_record` UPPER
    WHERE upper.del_flag = 0 
      AND 4 = 4 
      AND 5 = 5 
    GROUP BY machine_code) upper_latitude 
    ON weaving_record.`machine_code` = upper_latitude.machine_code 
  LEFT JOIN 
    (SELECT 
      meter_number,
      workshop_name,
      SUM(output_weft) output_weft_sum,
      SUM(output_meter) output_meter_sum,
      AVG(shift_efficiency) shift_efficiency_avg 
    FROM
      `mes-wmchis`.`shift_info` 
    WHERE shift_info.`del_flag` = 0 
      AND 6 = 6 
      AND 7 = 7 
    GROUP BY meter_number) info 
    ON weaving_record.`machine_code` = info.`meter_number` 
WHERE weaving_record.`del_flag` = 0 
  AND 1 = 1 
  AND 2 = 2 
  AND 3 = 3 
GROUP BY weaving_record.`machine_code`,
  weaving_record.`work_team` 
ORDER BY weaving_record.`create_date` ]]></xml-property>
